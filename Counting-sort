a = [0, 1, 2, 3, 2, 1, 2, 3, 3, 2, 4, 3, 5, 3, 2]
count=[0]*6 #список з шести нулів
for i in a: #переребираємо елементи списка а
    count[i] +=1 #у списку count і буде почергово брати значення списка а
print(count)
for i in range(6): #переребираємо індекси масива count(6)
    print(((str(i)+' ')*count[i]),end='')# цифра та кількість раз ця цифра зустрілась

